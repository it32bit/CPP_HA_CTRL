{
    "version": 4,
    "configurations": [
        {
            "name": "STM32",
            "configurationProvider": "ms-vscode.cmake-tools",
            "intelliSenseMode": "gcc-arm",
            "includePath": [
                "${workspaceRoot}/**",
                "${workspaceRoot}/Core/Inc",
                "${workspaceRoot}/Core/Src",
                "${workspaceRoot}/Drivers/stm32f4xx-hal-driver/Inc",
                "${workspaceRoot}/Drivers/stm32f4xx-hal-driver/Src",
                "${workspaceRoot}/Drivers/cmsis-device-f4/Include",
                "${workspaceRoot}/Drivers/CMSIS/Core/Include"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c17",
            "cppStandard": "c++20"
        }
    ]
}